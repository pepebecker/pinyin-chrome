{
	"manifest_version": 2,
	"name": "Pinyin Extension",
	"version": "0.1",
	"browser_action": {
		"default_icon": "img/icon-128.png",
		"default_title": "Pinyin",
		"default_popup": "index.html"
	},
	"icons": {
		"16": "img/icon-16.png",
		"48": "img/icon-48.png",
		"128": "img/icon-128.png"
	},
	"permissions": [
		"tabs",
		"http://*/",
		"https://*/"
	]
}
